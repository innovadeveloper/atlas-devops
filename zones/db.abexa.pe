; sudo nano /etc/bind/zones/db.abexa.pe
$TTL    1D
@       IN      SOA     ns1.abexa.pe. admin.abexa.pe. (
        3               ; Serial
        12h             ; Refresh
        15m             ; Retry
        3w              ; Expire
        2h  )           ; Negative Cache TTL

;       Registros NS

        IN      NS      ns1.abexa.pe.
ns1     IN      A       192.168.64.3
www     IN      A       192.168.64.3
sistemas	IN	A		192.168.1.99
identityserver-service	IN	A	192.168.1.4
artifactory-service		IN	A	192.168.1.4
portainerdev-service	IN	A	192.168.1.4
jenkinsdev-service		IN	A	192.168.1.4
registrydev-service		IN	A	192.168.1.4

; [end]
